<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/nodeFragment">
    <fragment
        android:id="@+id/nodeFragment"
        android:name="com.seytkalievm.denettesttree.presentation.node_fragment.NodeFragment"
        tools:layout="@layout/fragment_node" />
    <action
        android:id="@+id/action_global_nodeFragment"
        app:launchSingleTop="false"
        app:destination="@id/nodeFragment" >
        <argument
            android:name="Node"
            app:argType="string"
            android:defaultValue="null" />
    </action>
</navigation>